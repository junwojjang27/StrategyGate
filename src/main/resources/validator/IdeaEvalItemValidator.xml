<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE form-validation PUBLIC
	"-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.1//EN"
	"http://jakarta.apache.org/commons/dtds/validator_1_1.dtd">

<form-validation>
	<formset>
		<form name="ideaEvalItemVO">
			<field property="evalItemTitle" depends="required, maxByteLength">
				<arg0 key="항목이름"/>
				<arg1 key="20" resource="false"/>
				<var>
					<var-name>maxByteLength</var-name>
					<var-value>20</var-value>
				</var>
			</field>
			<field property="evalItemContent" depends="required, maxByteLength">
				<arg0 key="항목내용"/>
				<arg1 key="100" resource="false"/>
				<var>
					<var-name>maxByteLength</var-name>
					<var-value>100</var-value>
				</var>
			</field>
		</form>
	</formset>
</form-validation>