<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE form-validation PUBLIC
	"-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.1//EN"
	"http://jakarta.apache.org/commons/dtds/validator_1_1.dtd">

<form-validation>
	<formset>
		<form name="ideaEvalItemVO">
			<field property="evalItemTitle" depends="required, maxlength">
				<arg0 key="항목이름" resource="true"/>
				<arg1 key="20" resource="false"/>
				<var>
					<var-name>maxlength</var-name>
					<var-value>20</var-value>
				</var>
			</field>
			<field property="evalItemContent" depends="required, maxlength">
				<arg0 key="항목 내용" resource="true"/>
				<arg1 key="100" resource="false"/>
				<var>
					<var-name>maxlength</var-name>
					<var-value>100</var-value>
				</var>
			</field>
			<field property="weightId" depends="required, maxlength">
				<arg0 key="가중치" resource="true"/>
				<arg1 key="22" resource="false"/>
				<var>
					<var-name>maxlength</var-name>
					<var-value>22</var-value>
				</var>
			</field>
			<field property="particalTypeId" depends="required">
				<arg0 key="평가자구분" resource="true"/>
				<arg1 key="20" resource="false"/>
				<var>
					<var-name>maxlength</var-name>
					<var-value>20</var-value>
				</var>
			</field>

		</form>
	</formset>
</form-validation>

