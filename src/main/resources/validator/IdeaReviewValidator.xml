<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE form-validation PUBLIC
	"-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.1//EN"
	"http://jakarta.apache.org/commons/dtds/validator_1_1.dtd">

<form-validation>
	<formset>
		<form name="ideaReviewVO">
			<field property="state" depends="required">
				<arg0 key="진행상태(대기/진행/완료)" resource="true"/>
			</field>
			<field property="startDt" depends="maxByteLength, integer">
				<arg0 key="평가시작일자" resource="true"/>
				<arg1 key="8" resource="false"/>
				<var>
					<var-name>maxByteLength</var-name>
					<var-value>8</var-value>
				</var>
			</field>
			<field property="endDt" depends="maxByteLength, integer">
				<arg0 key="평가종료일자" resource="true"/>
				<arg1 key="8" resource="false"/>
				<var>
					<var-name>maxByteLength</var-name>
					<var-value>8</var-value>
				</var>
			</field>

		</form>
	</formset>
</form-validation>

