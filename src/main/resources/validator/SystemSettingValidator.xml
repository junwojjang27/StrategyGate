<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE form-validation PUBLIC
	"-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.1//EN"
	"http://jakarta.apache.org/commons/dtds/validator_1_1.dtd">

<form-validation>
	<formset>
		<form name="systemSettingVO">
			<field property="maxScore" depends="maxlength">
				<arg0 key="word.maxScore" resource="true"/>
				<arg1 key="1" resource="false"/>
				<var>
					<var-name>maxlength</var-name>
					<var-value>11</var-value>
				</var>
			</field>
			<field property="minScore" depends="maxlength">
				<arg0 key="word.minScore" resource="true"/>
				<arg1 key="1" resource="false"/>
				<var>
					<var-name>maxlength</var-name>
					<var-value>11</var-value>
				</var>
			</field>
		</form>
	</formset>
</form-validation>

